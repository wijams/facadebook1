QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,WAFA;AAGA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAHA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAUA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA3BA;AA4BA,IAAA,UA5BA,sBA4BA,IA5BA,EA4BA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KA/BA;AAgCA,IAAA,UAhCA,sBAgCA,IAhCA,EAgCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAzCA;AA0CA,IAAA,QA1CA,oBA0CA,IA1CA,EA0CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAMA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAPA,kDAQA,IARA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AAtDA;AAtBA,CAAA","sourcesContent":["<template>\n<div class=\"admin\">\n  <h1>The Admin Page!</h1>\n  <div class=\"heading\">\n      <div class=\"circle\">1</div>\n      <h2>Add an Item</h2>\n  </div>\n  <div class=\"add\">\n      <div class=\"form\">\n          <input v-model=\"title\" placeholder=\"Title\" />\n          <p></p>\n          <textarea v-model=\"description\" placeholder=\"Description\" />\n          <br>\n          <input type=\"file\" name=\"photo\" @change=\"fileChanged\" />\n          <button @click=\"upload\">Upload</button>\n      </div>\n      <div class=\"upload\" v-if=\"addItem\">\n          <h2>{{addItem.title}}</h2>\n          <img :src=\"addItem.path\" />\n      </div>\n  </div>\n  <div class=\"heading\">\n      <div class=\"circle\">2</div>\n      <h2>Edit/Delete an Item</h2>\n  </div>\n  <div class=\"edit\">\n      <div class=\"form\">\n          <input v-model=\"findTitle\" placeholder=\"Search\">\n          <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n            <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n            </div>\n          </div>\n      </div>\n      <div class=\"upload\" v-if=\"findItem\">\n          <input v-model=\"findItem.title\">\n          <p></p>\n          <textarea v-model=\"findItem.description\" />\n          <br />\n          <img :src=\"findItem.path\" />\n      </div>\n      <div class=\"actions\" v-if=\"findItem\">\n          <button @click=\"deleteItem(findItem)\">Delete</button>\n          <button @click=\"editItem(findItem)\">Edit</button>\n      </div>\n  </div>\n</div>\n</template>\n\n<script>\n    import axios from 'axios';\n    export default {\n        name: 'Admin',\n        data() {\n            return {\n                title: \"\",\n                description: \"\",\n                file: null,\n                addItem: null,\n                items: [],\n                findTitle: \"\",\n                findItem: null,\n            }\n        },\n        computed: {\n            suggestions() {\n                let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n                return items.sort((a, b) => a.title > b.title);\n            }\n        },\n        created() {\n            this.getItems();\n        },\n        methods: {\n            fileChanged(event) {\n                this.file = event.target.files[0]\n            },\n            async upload() {\n                try {\n                    const formData = new FormData();\n                    formData.append('photo', this.file, this.file.name)\n                    let r1 = await axios.post('/api/photos', formData);\n                    let r2 = await axios.post('/api/items', {\n                        title: this.title,\n                        description: this.description,\n                        path: r1.data.path\n                    });\n                    this.addItem = r2.data;\n                } catch (error) {\n                    console.log(error);\n                }\n            },\n            async getItems() {\n                try {\n                    let response = await axios.get(\"/api/items\");\n                    this.items = response.data;\n                    return true;\n                } catch (error) {\n                    console.log(error);\n                }\n            },\n            selectItem(item) {\n                this.findTitle = \"\";\n                this.findItem = item;\n            },\n            async deleteItem(item) {\n                try {\n                    await axios.delete(\"/api/items/\" + item._id);\n                    this.findItem = null;\n                    this.getItems();\n                    return true;\n                } catch (error) {\n                    console.log(error);\n                }\n            },\n            async editItem(item) {\n                try {\n                    await axios.put(\"/api/items/\" + item._id, {\n                        title: this.findItem.title,\n                        description: this.findItem.description,\n                    });\n                    this.findItem = null;\n                    this.getItems();\n                    return true;\n                } catch (error) {\n                    console.log(error);\n                }\n            },\n        }\n    }\n</script>\n\n<style scoped>\n    .image h2 {\n        font-style: italic;\n        font-size: 1em;\n    }\n\n    .heading {\n        display: flex;\n        margin-bottom: 20px;\n        margin-top: 20px;\n    }\n\n        .heading h2 {\n            margin-top: 8px;\n            margin-left: 10px;\n        }\n\n    .add,\n    .edit {\n        display: flex;\n    }\n\n    .circle {\n        border-radius: 50%;\n        width: 18px;\n        height: 18px;\n        padding: 8px;\n        background: #333;\n        color: #fff;\n        text-align: center\n    }\n\n    /* Form */\n    input,\n    textarea,\n    select,\n    button {\n        font-family: 'Montserrat', sans-serif;\n        font-size: 1em;\n    }\n\n    .form {\n        margin-right: 50px;\n    }\n\n    /* Uploaded images */\n    .upload h2 {\n        margin: 0px;\n    }\n\n    .upload img {\n        max-width: 300px;\n    }\n\n    /* Suggestions */\n    .suggestions {\n        width: 200px;\n        border: 1px solid #ccc;\n    }\n\n    .suggestion {\n        min-height: 20px;\n    }\n\n        .suggestion:hover {\n            background-color: #5BDEFF;\n            color: #fff;\n        }\n</style>"],"sourceRoot":"src/views"}]}